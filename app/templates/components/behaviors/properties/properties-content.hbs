{{behaviors/properties/default-content
  node=node
}}

{{#each node.structure.toggle as |toggle|}}
  {{component 'behaviors/properties/toggle-structure'
    toggle=toggle
    uri=node.uri
  }}
{{/each}}

{{#each node.structure.parameters as |parameter|}}
  {{component 'behaviors/properties/parameter-structure'
    parameter=parameter
    uri=node.uri
  }}
{{/each}}

{{#each node.structure.parameterSets as |set|}}
  {{component 'behaviors/properties/set-structure'
    set=set
    uri=node.uri
  }}
{{/each}}

{{#each node.structure.lists as |list|}}
  {{component 'behaviors/properties/list-structure'
    list=list
    uri=node.uri
  }}
{{/each}}

{{yield}}
