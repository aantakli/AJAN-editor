{{#if malformedQuery}}
<div local-class="block false">
  <i class="exclamation circle icon"></i> Malformed query
</div>
{{/if}}

{{#if toManyStatements}}
<div local-class="block many">
  <i class="exclamation triangle icon"></i> Too many statements
</div>
{{/if}}

{{#unless tableData}}
<div local-class="block empty">
  No results found
</div>
{{/unless}}

