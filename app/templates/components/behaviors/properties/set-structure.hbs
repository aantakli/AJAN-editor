{{#ui-accordion}}
  {{#ui/side-pane/title-wrapper
    title=set.title
    class="active title"
    isAccordion=true
  }}
    {{#if removeButtonAction}}
      {{component 'ui/buttons/remove-button'
        onClick=removeButtonAction
      }}
    {{else}}
      {{component 'ui/buttons/button-placeholder'}}
    {{/if}}
  {{/ui/side-pane/title-wrapper}}

  <div class="behavior side-pane set item-container content active">
    {{#each set.toggle as |toggle|}}
      {{component 'behaviors/properties/toggle-structure'
        class="toga"
        toggle=toggle
        uri=parentUri
        casualMode=casualMode
      }}
    {{/each}}

    {{#each set.parameters as |parameter|}}
      {{component 'behaviors/properties/parameter-structure'
        class="para"
        parameter=parameter
        uri=parentUri
        casualMode=casualMode
      }}
    {{/each}}

    {{#each set.parameterSets as |subset|}}
      {{component 'behaviors/properties/set-structure'
        class="sata"
        set=subset
        uri=parentUri
        casualMode=casualMode
      }}
    {{/each}}

    {{#each set.lists as |list|}}
      {{component 'behaviors/properties/list-structure'
      	class="lista"
        list=list
        uri=parentUri
        casualMode=casualMode
      }}
    {{/each}}

  </div>
{{/ui-accordion}}
